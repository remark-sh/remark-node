{
  "name": "@remark-sh/sdk",
  "version": "0.4.0",
  "description": "Node.js library for the Remark API",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remark-sdk/remark-node.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remark-sdk/remark-node/issues"
  },
  "homepage": "https://github.com/remark-sdk/remark-node#readme",
  "devDependencies": {
    "@types/node": "18.19.80",
    "tsup": "7.2.0",
    "typescript": "5.8.2"
  },
  "packageManager": "bun@1.2.8"
}